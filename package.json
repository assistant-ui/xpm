{
  "name": "@assistant-ui/xpm",
  "version": "0.0.7",
  "description": "Universal package manager wrapper that automatically detects and uses the right package manager",
  "main": "dist/index.js",
  "bin": {
    "xpm": "./dist/cli.js",
    "xpx": "./dist/xpx-cli.js"
  },
  "scripts": {
